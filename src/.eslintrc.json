{
  "env": {
    "node": false,
    "commonjs": true,
    "browser": true
  },
  "globals": {
    "__env": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": ["react"],
  "extends": ["plugin:react/recommended"],
  "rules": {}
}
